function init_lvl(wi,wj)
	local si,sj=wi+128,wj+128
	
	printh("creating room "..
		wi..","..wj.." ("..
		si..","..sj..")")
	srand((sj<<8)+si)
	
	lvl={}
	for j=0,15 do
	for i=0,15 do
		add(lvl,{s=1,x=i*8,y=j*8})
	end end
	
	shdw={}
	
end
