					local d=drs[i]
					local yy=y+d[2]
					local zz=z+d[3]
					local xx=x+d[1]
				//loga({i,x,z,xx,zz,lvl[zz][xx]})
				
					if (
									yy<1 or yy>#lvl or
									zz<1 or zz>#lvl[y] or
									xx<1 or xx>#lvl[y][z]
								) or
