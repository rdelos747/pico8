function update_cam()
	camz=ppz
	camx=ppx
	camy=ppy
end

function update_player()
	
	if(btn(⬅️))ppx-=1
	if(btn(➡️))ppx+=1
	if(btn(⬆️))ppz+=1
	if(btn(⬇️))ppz-=1
	if(btn(❎))ppy-=1
	if(btn(🅾️))ppy+=1

	update_cam()
end
