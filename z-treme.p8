		local x=(k%5)
		local z=flr(k/5)*5
		//local imin=
		local drs={
			{0,0,1}, 			//check front
			{0,0,-1}, 		//check back
			{-1,0,0}, 		//check left
			{1,0,0}, 			//check right
			//{0,-1,0}, //check up
			//{0,1,0}, 	//check down
		}
		
		if lvl[i+j+1]
	end
	]]--
	
	local drs={
		{0,0,-1}, 		//check front
		//{0,0,-1}, 		//check back
		{-1,0,0}, 		//check left
		{1,0,0}, 			//check right
		{0,-1,0}, 		//check up
		//{0,1,0}, 	//check down
	}
	
	for y=1,#lvl do
		for z=1,#lvl[y] do
