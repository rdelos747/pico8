	for j=max(0,my-1),min(15,my+1) do
	for i=max(0,mx-1),min(15,mx+1) do		
		for f in all(lrs) do
			local s=mget(i,j)
			if fget(s,f) then
				--[[
				local b={
					x=i*8+4,y=j*8+4,
					w=8,h=8,s=s
				}
				]]--
				local b=obj(
					i*8+4,j*8+4,8,8,{s=s}
				)
				if col_bb(a,b) then
					return b
				end
			end
		end
	end end
