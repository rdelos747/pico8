pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
--constants
p_accel=0.065 --fall accel
p_f_max=2   --fall spd max
p_j_max=-1.5  --jump spd max
p_j_fre=2   --num free jumps

function obj(x,y,w,h)
	return {x=x,y=y,w=w,h=h}
end

--player vars
pp=obj(64,64,3,5)
pvy=0  			--vel y
pdrx=1 			--dir x
pdry=0 			--dir y
pmt=0  			--move time
pjmp=0 			--current jump
pjp=false --jump pressed
ptank=2 		--num water tanks
pwater=3 	--amt water

--other vars
hudt=0 --hud time


function _init()
	printh("=====start=====")
end

function _draw()
	cls()
	map(0,0)
	
	draw_player()
	draw_hud()
end

function _update60()
	update_player()
	
	hudt-=0.1
	if(hudt<0)hudt=2
end

function draw_bb(o)
	rect(
		o.x-o.w/2,
		o.y-o.h/2,
		o.x+o.w/2,
		o.y+o.h/2,
		8)	
end

function col_bb(a,b)
	local ax=a.x-a.w/2
	local ay=a.y-a.h/2
	local bx=b.x-b.w/2
	local by=b.y-b.h/2
	return ax<=bx+b.w and
		ax+a.w>=bx and ay<=by+b.h and
		ay+a.h>=by
end

function place_free(o,ox,oy)
	local a={
		x=o.x+ox,y=o.y+oy,w=o.w,h=o.h
	}
	local free=true
	for j=0,15 do
	for i=0,15 do
		if fget(mget(i,j),0) then
			local b={
				x=i*8+4,y=j*8+4,
				w=8,h=8
			}
			if col_bb(a,b) then
				free=false
			end
		end
	end end
	return free
end
-->8
--player
function draw_player()
	sspr(
		8,pdry*4+4,
		4,4,
		pp.x-2,pp.y-3,
		4,4,
		pdrx==-1
	)
	
	sspr(
		12,flr(pmt)%2*2,
		4,2,
		pp.x-2,pp.y+1,
		4,2,
		pmt<2
	)	
	//draw_bb(pp)
end

function update_player()
	local move=false
	pdry=0
	if(btn(➡️))move,pdrx=true,1
	if(btn(⬅️))move,pdrx=true,-1
	if(btn(⬆️))pdry=-1
	if(btn(⬇️))pdry=1
	
	if move then
		//pmt-=0.15
		if(pmt<0)pmt=4
		if place_free(pp,pdrx,0) then
			pp.x+=0.5*pdrx
		end
	else
		pmt=0
		pp.x=flr(pp.x)
	end
	
	--jump
	if btn(🅾️) then
		print(pjmp)
		if not pjp and pjmp<p_j_fre then
			pjp=true
			pvy=p_j_max
			pjmp+=1
		end
	else
		pjp=false
	end
	
	--fall
	pvy=min(p_f_max,pvy+p_accel)
	if pvy<0 then 
		if not place_free(pp,0,-2) then
			pvy=0.5
		end
	elseif not place_free(pp,0,max(1,pvy)) then
		if pvy>p_accel then
			printh("landed")
		end
		pp.y=(flr(pp.y/8)*8)+5
		pvy=0
		pjmp=0
	else
		printh(flr(pp.y).." "..pp.y)
	end
	pp.y+=pvy
	
	//pp.y=flr(pp.y)+0.5
	//pp.x=flr(pp.x)+0.5
end
-->8
--hud/general

function draw_hud()
	rectfill(0,0,127,6,0)
	for i=0,p_j_fre-1 do
		sspr(44,0,3,6,i*5,0)
	end
	for i=ptank,1,-1 do
		local nw=mid(
			0,
			pwater-(ptank*3-i*3),
			3)
		if nw==0 then
			sspr(0,8,6,6,8+(i*6),1)
		else
		--[[
			spr(
				20+(nw*2)-(flr(hud.tm)%2),
				hud_j_x+16+(i*6),
				122)
		]]--
			sspr(
				16+4*(nw+2)-(flr(hudt)%2)*4,0,
				5,6,8+i*6,1)
		end
	end
end
