--map functions

function init_lvl()
	waters={}
	
	local mx,my=flr(pp.x/128),flr(pp.y/128)
	
	for j=0,15 do
	for i=0,15 do
		local x=mx*128+i*8
		local y=mx*128+j*8
		local s=mget(mx+i,my+j)
		if fget(s,7) then
			add(waters,{
				x=x+4,y=y+4,w=4,h=8
			})
			mset(mx+i,my+j,0)
		end
	end end
end

function draw_lvl()
	for w in all(waters)do
		sspr(
			14*8+flr(uits)*4,0,
			4,8,
			w.x-2,w.y-4)
		//draw_bb(w)
	end
end
