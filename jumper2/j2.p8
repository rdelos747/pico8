--hud/general

function draw_hud()
	rectfill(0,0,127,6,0)
	for i=0,p_j_fre-1 do
		if pjmp>i then
			pal(7,1)
			pal(12,1)
		end
		sspr(44,0,3,6,i*5,0)
		pal()
	end
	for i=1,ptank do
		local nw=mid(
			0,
			pwater-(ptank*3-i*3),
			3)
		if nw==0 then
			sspr(0,8,6,6,8+(i*6),1)
		else
			sspr(
				16+8*(nw-1)+(flr(uits)%2)*4,0,
				5,5,8+i*6,1)
		end
	end
end
