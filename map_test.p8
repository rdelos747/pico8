pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
hb={cx=4,cy=4,w=8,h=8}
c_x=0
c_y=0
p_x=30
p_y=0

function _draw()
	cls()
	camera(c_x,c_y)
	m_x=flr(c_x/8)-1
	m_y=flr(c_y/8)-1
	map(m_x,m_y,m_x*8,m_y*8,18,18)
	spr(3,p_x,p_y)
	spr(4,p_x+4,p_y+7)
end

function _update()
	dx,dy=0,0
	if(btn(0))dx=-1
	if(btn(1))dx=1
	if(btn(2))dy=-1
	if(btn(3))dy=1
	local o=mget((p_x+4)/8,(p_y+4)/8)
	if(fget(o,0)and dx==1)then
		dx=0
	end
	p_x+=dx
end
