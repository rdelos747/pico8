-- shrimp
ver="0.0.7"

acx=0.2 --x accel
p_x_max=1.5 --x speed max

acy=0.2 --y fall accel (grav)
acyj=0.1 --y float accel
p_f_max=2 --fall speed max
p_j_max=-2 --float speed max
p_c_ref=100 --charge refresh spd
p_c_max=80 --charge max


fric=0.1 --friction

crl=0

function _init()
	printh("=======start=======")
	reset_lvl()
end

function _draw()
	cls(0)
	map(0,0)
	//loga({cmi*8,camx,(cmi+cmw)*8-64})
	camx=mid(cmi*8,camx,(cmi+cmw)*8-128)
	camy=mid(cmj*8,camy,(cmj+cmh)*8-128)
	camera(flr(camx),flr(camy))
	
	for t in all(tris)do
		local c=t.test and 11 or 7
		for i=1,3 do
			t1=t[i]
			t2=t[i%3+1]
			//pset(t[i][1],t[i][2],c)
			//line(
			//	t1[1],t1[2],
			//	t2[1],t2[2],c)
		end
	end
	
	draw_effs()
	draw_enemies()
	
	draw_player()
	
	//print(ver,0,0,1)
	print("scr:"..scr,camx,camy,7)
end

function _update()
	camx=flr(pp.x)-64
	camy=flr(pp.y)-64
	l_uits,uits=uits,(uits+0.1)%30
	l_uitf,uitf=uitf,(uitf+0.2)%30
	update_effs()
	update_enemies()
	update_player()
end

function reset_lvl()
	reload(0x1000,0x1000,0x2000)
	uits,l_uits=-0.1,-1 --ui time slow
	uitf,l_uitf=-0.1,-1 --ui time fast
	
	for t in all(tris)do
		del(tris,t)
	end
	
	for e in all(enemies)do
		del(enemies,e)
	end
	
	for e in all(effs)do
		del(effs,e)
	end
	
	ppa,pps=0,0
	pvx,pvy=0,0
	ppc=p_c_max
	pp_ong=false
	pr0ll=true
	tris={}
	scr=0
	
	lvl=lvls[crl+1]
	init_lvl()
	get_tris()
	
	printh("cur lvl")
	loga(lvl)
	loga({cmi,cmj,cmw,cmh})
end
