function update_player()
	ppf=false -- player flying
	if btn(ðŸ…¾ï¸) and ppc>0 then
		-- jumping
		pvy=max(pvy-acyj,p_j_max)
		ppc-=1
		ppf=true
		proll=false
		add_fdot(pp.x,pp.y)
	else
		-- gravity
		pvy=min(pvy+acy,p_f_max)
	end
	
	if not pp_ong or btn(âŽ) then
		if btn(âž¡ï¸) then
			pvx=min(pvx+acx,p_x_max)
		elseif btn(â¬…ï¸) then
			pvx=max(pvx-acx,p_x_max*-1)
		end
	end	
	
	pp.x+=pvx
	pp.y+=pvy
	//loga({pvx,pvy})
	
	-- speed
	pps=sqrt(pvx^2+pvy^2)	
	-- angle
	ppa=atan2(pvx,pvy)
	
	-- triangle collision
	local tch_tri=false
	for t in all(tris)do
		-- resolve the collision
		t.test=false
		local ox,oy=0,0
		while c_in_tri(
				pp.x+ox,
				pp.y+oy,
				t
		) do
			t.test=true
			tch_tri=true
			proll=true
			ox+=cos(t.n)
			oy+=sin(t.n)
			printh("here tri")
		end
		
		-- find new direction
		if ox!=0 or oy!=0 then
			pp.x+=ox
			pp.y+=oy
			
			reflect(t.n,t.frx,t.fry,proll,"tri")
		end
	end
	
	-- vert solid colision
	if not tch_tri then
		pp_ong=false
		local cv=c_on_sol(pp.x,pp.y)
		if cv then
			pp.y=(flr(pp.y/8)*8)+4
			//printh("here vert")
			local f=sols_m[cv.s]
			reflect(0.25,f[1],f[2])
			if cv.y>pp.y then
				pp_ong=true
				proll=true
			end
		end
	end
	
	-- horz solid colision
	local ch=c_on_sol(pp.x+pvx,pp.y)
	if ch and 
				abs(ch.y-pp.y)<=5 then
		pp.x=(flr(pp.x/8)*8)+4
		printh("here hh")
		local fr=sols_m[ch.s]
		//loga({abs(ch.y-pp.y)})
		reflect(0.5,fr[1],fr[2])
	end
	
	-- recharge
	if pp_ong then
		if pps<=0.5 then
			if ppc<p_c_max then
				scr+=1
			end
			ppc=min(ppc+p_c_ref,p_c_max)
		end
	end
	
	-- die off screen
	if pp.y>cmy+cmh*8+8 then
		reset_lvl()
	end
	
	-- touch enemy
	for e in all(enemies)do
		if col_bb(pp,e) then
			reset_lvl()
		end
	end
end

function reflect(n,frx,fry,roll,db)
	local d=(ppa+0.5)%1
	//local ra=a-t.n
	local d2=d-n
	local ra=n-d2
	db=db!=nil and db or "nil"
	loga({
		db,
		ppa,ppa+0.5,n,
		d,d2,ra
	})
	
	//pps*=1-0.5
				
	//loga({frx,fry,ra,cos(ra)})

	
	pvx=cos(ra)*pps
	pvy=sin(ra)*pps
	
	pvx*=1-frx
	pvy*=1-fry
	
	//pvx-=cos(ra)*0.1
	//pvy+=sin(ra)*0.9
	
	//pvx*=cos(ra)*(1-1)//*(3)*sgn(pvx)
	//pvy*=sin(ra)//*(1)//*sgn(pvx)
	
	//pvx*=cra*(1-frx)//*sgn(cra)
	//pvy*=sra*(1-fry)//*sgn(sra)
	//loga({pvx,pvy})
	
end

-- test points around circle
-- for triangle collision
function c_in_tri(x,y,t)
	for i=0,15 do
		local a=i/16
		local ox=cos(a)*4
		local oy=sin(a)*4
		local f=pt_in_tri(
			x+ox,
			y+oy,
			t
		)
		if(f)return f
	end
	return nil
end

function c_on_sol(x,y)
	for i=0,15 do
		local a=i/16
		local ox=cos(a)*4
		local oy=sin(a)*4
		
		local mi=flr((x+ox)/8)
		local mj=flr((y+oy)/8)
		
		for j=min(cmj+cmh,mj+1),max(cmj,mj-1),-1 do
		for ii=mi-1,mi+1 do
			local i=ii%cmw+cmi
			local s=mget(i,j)
			if fget(s,0) then
				local b=obj(
					i*8+4,
					j*8+4,
					8,8
				)
				b.s=s
				if pt_bb(x+ox,y+oy,b) then
					return b
				end
			end
		end end
		
	end
	return nil
end
