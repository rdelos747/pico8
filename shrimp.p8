function init_lvl()
	enemies={}
	effs={}
	
	cmi,cmj=lvl[1],lvl[2]
	cmw,cmh=lvl[3],lvl[4]
	cmx,cmy=cmi*8,cmj*8
	
	for j=cmj,cmj+cmh-1 do
	for i=cmi,cmi+cmw-1 do
		local s=mget(i,j)
		local fl=fget(s)
		//local sn=mget(cmi+i,cmj+max(j-1,0))
		//local ss=mget(cmi+i,cmj+min(j+1,15))
		//local sw=mget(cmi+max(i-1,0),cmj+j)
		//local se=mget(cmi+min(i+1,15),cmj+j)

		local x,y=cmx+i*8,cmy+j*8
		
		if fl==0 then
			mset(i,j,0)
			
			if s==1 then
				-- player
				pp={x=i*8+4,y=j*8+4,w=8,h=8}	
				printh("found")
			elseif s==16 then
				-- spark ball
				add_enemy(
					x+4,
					y+4,
					7,7,
					draw_spark,
					update_spark
				)
			end
		end
	end end
end

function get_tris()
	//local ii,jj=cmi*16,cmj*16
	for j=cmj,cmj+cmh-1 do
	for i=cmi,cmi+cmw-1 do
		local s=mget(i,j)
		if tris_m[s]!=nil then
			//pset(i*8,j*8,7)
			//add(sols,{
			local t={}
			//printh(#tris)
			local td=tris_d[tris_m[s]]
			for k=1,3 do
				add(t,{
					i*8+td[k][1],
					j*8+td[k][2]
				})
			end
			t.n=td[4]
			t.frx=t_fric
			t.fry=t_fric
			add(tris,t)
		end
	end end
end

